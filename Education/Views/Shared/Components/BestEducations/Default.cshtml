@using Education.Models.Home
@model EducationsVM

<div class="footer_right_box">
    <p class="footer_title">Լավագույն դասընթացներ</p>
    @foreach (var item in Model.GetUIBestEducation)
    {
        <a asp-controller="Home" asp-action="Education" asp-route-educationsid="@item.EducationsId">
            <p>@item.UName</p>
        </a>
    }
</div>